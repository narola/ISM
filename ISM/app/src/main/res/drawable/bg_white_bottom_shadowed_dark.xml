<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape android:shape="rectangle">
			<!-- 20 opacity -->
			<solid android:color="#14D9D9D9" />
		</shape>
	</item>
	<item android:bottom="@dimen/dp2">
		<shape android:shape="rectangle">
			<!-- 40 opacity -->
			<solid android:color="#28D9D9D9" />
		</shape>
	</item>
	<item android:bottom="@dimen/dp3">
		<shape android:shape="rectangle">
			<!-- 100 opacity -->
			<solid android:color="#64D9D9D9" />
		</shape>
	</item>
	<item android:bottom="@dimen/dp5">
		<shape android:shape="rectangle">
			<!-- 150 opacity -->
			<solid android:color="#96D9D9D9" />
		</shape>
	</item>
	<item android:bottom="@dimen/dp6">
		<shape android:shape="rectangle">
			<!-- 256(full) opacity -->
			<solid android:color="#D9D9D9" />
		</shape>
	</item>
	<item android:bottom="@dimen/dp8">
		<shape android:shape="rectangle">
			<solid android:color="@color/white" />
		</shape>
	</item>
</layer-list>